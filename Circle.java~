import java.util.Scanner;
public class Circle
{
  public static void main(String[] args)
  {
    float radius,area;
    Scanner sc = new Scanner(System.in);
    System.out.print("Enter the radius: ");
    radius = sc.nextFloat();
    area = Math.PI * Math.pow(radius,2);
    
    System.out.println("The area = " + area);